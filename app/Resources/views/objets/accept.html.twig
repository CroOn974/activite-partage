{% extends 'base.html.twig' %}

{% block body %}
    <div class="container m-t-3 p-t-1">
        <h1 class="p-b-1">Objet obtenue</h1>

        <table class="table">
            <thead class="thead-inverse">
            <tr>
                <th>Nom de l'objet</th>
                <th>Nom</th>
                <th>Prenom</th>
                <th>Adresse</th>
                <th>Tel</th>
            </tr>
            </thead>
            <tbody>
            {% for choix2 in choix %}
                <tr>
                    <td>{{choix2.objets.nom}}</td>
                    <td>{{choix2.objets.particulier.nom}}</td>
                    <td>{{choix2.objets.particulier.prenom}}</td>
                    <td>{{choix2.objets.particulier.adresse}}</td>
                    <td>{{choix2.objets.particulier.tel}}</td>
                </tr>

            {% endfor %}
            </tbody>
        </table>
    </div>
{% endblock %}
